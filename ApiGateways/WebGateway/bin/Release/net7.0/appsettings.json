{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "OpenApi": {
    "Endpoint": {
      "Name": "API Endpoint V1"
    },
    "Document": {
      "Description": "Gateway for Web Clients",
      "Title": "Gateway for Web Clients",
      "Version": "v1"
    },
    "Auth": {
      "ClientId": "BIVN.FixedStorage",
      "AppName": "FixedStorage Swagger UI"
    }
  },
  "Identity": {
    "Url": "http://localhost:9223",
    "Audience": "BIVN.FixedStorage",
    "Scopes": {
      "BIVN.FixedStorage": "Gateway for Web Clients"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "s-short": {
        "ClusterId": "storage",
        "Match": {
          "Path": "s/{**catch-all}"
        }
      },
      "s-long": {
        "ClusterId": "storage",
        "Match": {
          "Path": "api/storage/{**catch-all}"
        }
      },
      "i-short": {
        "ClusterId": "identity",
        "Match": {
          "Path": "api/identity/{**catch-all}"
        }
      },
      "inventory-long": {
        "ClusterId": "inventory",
        "Match": {
          "Path": "api/inventory/{**catch-all}"
        }
      },
      "i-static": {
        "ClusterId": "identity-wwwroot-images",
        "Match": {
          "Path": "/images/identity/{**catch-all}"
        }
      },
      "i-inventory-static": {
        "ClusterId": "inventory-wwwroot-images",
        "Match": {
          "Path": "/images/inventory/{**catch-all}"
        }
      },
      "error-investigation-long": {
        "ClusterId": "error-investigation",
        "Match": {
          "Path": "api/error-investigation/{**catch-all}"
        }
      },
      "i-error-investigation-static": {
        "ClusterId": "inventory-wwwroot-images",
        "Match": {
          "Path": "/images/ErrorInvestigation/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "storage": {
        "Destinations": {
          "destination0": {
            "Address": "http://localhost:9101"
          }
        }
      },
      "identity": {
        "Destinations": {
          "destination0": {
            "Address": "http://localhost:9223"
          }
        }
      },
      "identity-wwwroot-images": {
        "Destinations": {
          "destination0": {
            "Address": "http://localhost:9223"
          }
        }
      },
      "inventory": {
        "Destinations": {
          "destination0": {
            "Address": "http://localhost:9201"
          }
        }
      },
      "inventory-wwwroot-images": {
        "Destinations": {
          "destination0": {
            "Address": "http://localhost:9201"
          }
        }
      },
      "error-investigation": {
        "Destinations": {
          "destination0": {
            "Address": "http://localhost:9201"
          }
        }
      },
      "error-investigation-wwwroot-images": {
        "Destinations": {
          "destination0": {
            "Address": "http://localhost:9201"
          }
        }
      }
    }
  },
  "Urls": {
    "Storage": "http://localhost:9101",
    "Identity": "http://localhost:9223",
    "Inventory": "http://localhost:9201"
  }
}